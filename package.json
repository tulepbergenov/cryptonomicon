    "eslint-plugin-vue": "^9.28.0",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "stylelint": "^16.9.0",
    "stylelint-config-rational-order-fix": "^0.1.9",
